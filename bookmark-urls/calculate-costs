javascript:calculateEstimate();function calculateEstimate(){var a={en:{pleaseEnter:()=>"Please enter the cost per hour in \u20AC:",costResult:a=>`Estimatet cost for displayed issues: ${a} €.`,notANumber:()=>"This is not a number. Please try again."},de:{pleaseEnter:()=>"Bitte die Kosten f\xFCr eine Arbeitsstunde in \u20AC eingeben:",costResult:a=>`Ungefähre Kosten für angezeigte Tickets: ${a} €.`,notANumber:()=>"Das ist leider keine g\xFCltige Zahl. Versuch's nochmal."}},b=document.documentElement.lang in a?a[document.documentElement.lang]:a.en,c=null!==document.querySelector(".navigator-container");if(!c)return void alert("Error: Please go to the Jira issue navigator.");var d=null!==document.querySelector(".details-layout");if(d)return void alert("Error: Please change view type to list.");var e=document.querySelectorAll(".aggregatetimeestimate");if(1>e.length)return void alert("Error: Please enable the column '\u03A3 Remaining Estimate.'");var f=[],g=0,h={week:2400,day:480,hour:60,minute:1};e.forEach(a=>{var b=a.innerHTML;if(b){var c=b.split(",");f.push(...c)}}),f.forEach(a=>{a=a.trim(),a.endsWith("s")&&(a=a.substring(0,a.length-1));var b=a.split(" "),c=parseInt(b[0]),d=h[b[1]];g+=c*d});var i,j=g/60;if(i=100*parseFloat(window.prompt(b.pleaseEnter(),9e3/100)),isNaN(i))return void alert(b.notANumber());var k=(i*j/100).toFixed(2);alert(b.costResult(k))}