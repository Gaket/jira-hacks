javascript:(function(){'use strict';(function(){var a=document.getElementById("summary-val")||document.querySelector(".issuerow.focused > .summary"),b=document.querySelector("#key-val")||document.querySelector("#issuekey-val")||document.querySelector(".issuerow.focused > .issuekey"),c=b.textContent,d=document.createTextNode(`${c} `),e=document.createRange();e.selectNode(a),e.insertNode(d);window.getSelection&&(window.getSelection().addRange(e),document.execCommand("copy"),d.parentNode.removeChild(d),window.getSelection&&(window.getSelection().empty?window.getSelection().empty():window.getSelection().removeAllRanges&&window.getSelection().removeAllRanges()),JIRA.Messages.showSuccessMsg("Issue Key & Summary copied successfully to clipboard \uD83D\uDCCB."))})()})();